<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
 	xmlns:gap="http://phonegap.com/ns/1.0"
 	id="br.fib.lp5.Aula1" Version="1.0.0">
  	<name>LP5-Aula1</name>
 	<description>LP5 Aula 1</description>
  	<author href="http://fibbauru.br"
 	email="allanrichard_silva@hotmail.com">
 	Allan
 </author>
	<preference name="orientation" value="portrait"> </preference>
	<preference name="fullscreen" value="true"> </preference>
	<preference name="permissions" value="none"> </preference>
	<preference name="android-minSdkVersion" value="16"> </preference>
	<icon src="imagens/icon.png"> </icon>
	<platform name="android">
		<gap:splash src="imagens/splash.png" density="hdpi"> </gap:splash>
		<gap:splash src="imagens/splash.png" density="ldpi"/> </gap:splash>
		<gap:splash src="imagens/splash.png" density="mdpi"/> </gap:splash>
		<gap:splash src="imagens/splash.png" density="xhdpi"/> </gap:splash>
	</platform>
	<preference name="SplashScreenDelay" value="30000" />
	
</widget>